<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/go_home"
        android:layout_width="86dp"
        android:layout_height="54dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:drawableTop="@drawable/home_icon"
        android:contentDescription="홈 버튼"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/go_mypage"
        android:layout_width="86dp"
        android:layout_height="54dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:drawableTop="@drawable/mypage_icon"
        android:contentDescription="마이페이지 버튼"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/page_name"
        android:layout_width="174dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="44dp"
        android:text="비인가 사용자"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/go_home"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/unperson_iamge"
        android:layout_width="369dp"
        android:layout_height="370dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="144dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_background" />

    <Button
        android:id="@+id/img_store"
        android:layout_width="192dp"
        android:layout_height="63dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="24dp"
        android:text="사진 저장"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unpoerson_date" />

    <TextView
        android:id="@+id/unpoerson_date"
        android:layout_width="336dp"
        android:layout_height="45dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="12dp"
        android:text="2024.12.31 15:12:32"
        android:textSize="25sp"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unperson_iamge" />
</androidx.constraintlayout.widget.ConstraintLayout>